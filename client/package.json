{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "parse": "node ./src/helpers/extendParser.js",
    "build-css": "node-sass-chokidar src/ -o src/",
