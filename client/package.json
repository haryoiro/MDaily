    "@udecode/slate-plugins": "^0.66.1",
    "axios": "^0.19.2",
    "cra-sass": "^0.0.5",
    "draft-js": "^0.11.6",
    "megadraft": "^0.7.2",
    "node-sass-chokidar": "^1.5.0",
    "npm-add-script": "^1.1.0",
    "npm-run-all": "^4.1.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-draft-wysiwyg": "^1.14.5",
